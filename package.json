{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean:install": "npm run clean && npm install",
    "clean:start": "npm run clean:install && npm start",
    "copy:public": "cp -a src/public/. dist/",
    "copy:dep": "node static.copy",
    "compile:vendor": "rollup -c rollup.config.vendor.js && tsc --out ./dist/vendor.js --target es5 --allowJs dist/vendor.es2015.js || true",
    "compile:src": "npm run lint && rollup -c rollup.config.js && tsc --out ./dist/bundle.js --target es5 --allowJs dist/bundle.es2015.js || true",
    "compile:sass": "npm run compile:sass-global && npm run compile:sass-src",
    "compile:sass-global": "node-sass --quiet --include-path scss src/style/ -o dist/style/",
    "compile:sass-src": "node-sass --quiet --include-path scss -r src/app/ -o src/app/ && npm run compile:src",
    "uglify:vendor": "uglifyjs dist/vendor.js --screw-ie8 --compress --mangle --output dist/vendor.min.js",
    "uglify:src": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build:clean": "rimraf dist && npm run copy:public && npm run copy:dep && npm run compile:sass && npm run compile:vendor && npm run compile:src",
    "build:prod": "rimraf dist && npm run copy:public && npm run copy:dep && npm run compile:sass && npm run compile:vendor && npm run compile:src && npm run uglify:vendor && npm run uglify:src",
    "build:src": "npm run lint && npm run compile:src",
    "watch:public": "nodemon -w src/public -x \"npm run copy:public\"",
    "watch:ts": "nodemon -e ts -x \"npm run build:src\"",
    "watch:sass-global": "nodemon -w src/style -e scss -x \"npm run compile:sass-global\"",
    "watch:sass-src": "nodemon -w src/app -e scss -x \"npm run compile:sass-src\"",
    "watch": "npm-run-all --parallel watch:*",
    "dev:server": "cd dist && live-server --entry-file=index.html --port=4200 --ignorePattern=.*\\.es2015.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "tslint --force \"src/app/*.ts\"",
    "e2e": "protractor",
    "e2e:live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "start": "npm run build:clean",
    "postinstall": "npm run webdriver:update && typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "codelyzer": "0.0.26",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "0.1.1",
    "karma-webpack": "1.7.0",
    "live-server": "^1.1.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "protractor": "^3.1.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.1",
    "rollup": "^0.33.0",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-angular": "^0.2.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-includepaths": "^0.1.5",
    "rollup-plugin-node-resolve": "^1.7.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "5.0.0-beta.6",
    "rxjs-es": "^5.0.0-beta.11",
    "shelljs": "^0.7.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.4.0",
    "typescript": "^1.9.0-dev.20160625-1.0",
    "typings": "^1.3.0",
    "uglifyjs": "^2.4.10"
  }
}
